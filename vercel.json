{
  "buildCommand": "node inject-env.js",
  "outputDirectory": ".",
  "framework": null,
  "headers": [
    {
      "source": "/scripts/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate, no-cache" }
      ]
    },
    {
      "source": "/styles/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate, no-cache" }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    },
    {
      "source": "/(.+)\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2)$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate, no-cache" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
